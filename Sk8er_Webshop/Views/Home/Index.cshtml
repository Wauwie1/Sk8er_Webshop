
@{
    ViewData["Title"] = "Home";
    Layout = "_Layout";
}

<!--Carousel-->
<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="https://media1.popsugar-assets.com/files/thumbor/y6vjD4SyTkQJ3wisIidTp3x2OgA/fit-in/1024x1024/filters:format_auto-!!-:strip_icc-!!-/2013/03/12/1/498/4981322/29bac0cd783876e2_VA0413MIRANDA08/i/Miranda-Kerr-Vogue-Australia-April-2013-Cover-Shoot.jpg" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://www.texasmonthly.com/wp-content/uploads/2012/03/vogue-1250x0-c-default.png" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://static.adweek.com/adweek.com-prod/wp-content/uploads/2017/08/jennifer-lawrence-vogue-september-PAGE-2017.jpg" alt="Third slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
    
    <!--Jumbotron-->
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <div class="line-vertical">
                <h1>Fashion starts here</h1>

                <p class="lead indent">De laatste trends in streatwear vind je bij Sk8er, met merken als Daily Paper, Vans en HUF</p>
            </div>
        </div>
    </div>
</div>

